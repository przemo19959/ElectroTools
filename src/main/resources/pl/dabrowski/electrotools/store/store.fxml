<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>

<AnchorPane fx:id="anchor" prefHeight="200.0"
	prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="pl.dabrowski.electrotools.store.StoreController">
	<Rectangle fx:id="background" fill="rgb(69,69,69)"
		width="${anchor.prefWidth}" height="${anchor.prefHeight}" />
	<ScrollPane fx:id="scroll" prefHeight="${anchor.prefHeight}"
		prefWidth="${anchor.prefWidth}" translateY="10">
		<Label fx:id="info" text="" />
	</ScrollPane>
</AnchorPane>

